<DataSource 
	ID="adminassistants"
    dropExtraFields="true"
    serverType="generic"
    mappedBeanClass="com.skyquill.minder.model.AdminAssistant"
>
	<fields>
		<field name="id" 			type="text"  hidden="true" primaryKey="true"/>
		<field name="firstName" 	type="text"></field>
		<field name="lastName" 		type="text"></field>
		<field name="emailAddress" 	type="text"></field>
		<field name="officePhone" 	type="text"></field>
		<field name="cellPhone" 	type="text"></field>
		<field name="auditDateTime" type="datetime"></field>
		<field name="auditUserId" 	type="text"></field>
		<field name="version" 		type="number"></field>
	</fields>
    <serverObject lookupStyle="spring" bean="genericJdoDao"/>
</DataSource>