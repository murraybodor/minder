<DataSource 
	ID="metadata"
    dropExtraFields="true"
    serverType="generic"
    mappedBeanClass="com.skyquill.minder.model.MetaData"
>
	<fields>
		<field name="id" 			type="text"  hidden="true" primaryKey="true"/>
		<field name="metaKey" 		type="text"></field>
		<field name="strValue" 		type="text"></field>
		<field name="dateValue" 	type="date"></field>
		<field name="numValue" 		type="number"></field>
		<field name="booleanValue" 	type="number"></field>
		<field name="metaDescription" type="text"></field>
		<field name="metaSuppInfo" 	type="text"></field>
		<field name="auditDateTime" type="datetime"></field>
		<field name="auditUserId" 	type="text"></field>
		<field name="version" 		type="number"></field>
	</fields>
    <serverObject lookupStyle="spring" bean="genericJdoDao"/>
</DataSource>